<script type="text/javascript">
        // <![CDATA[

var twister_view_dpv_so;	   


function twister_view_dpv_loadSwf_Core(swfLocation, swfWidth, swfHeight, flashVersion, bgColor, swfParams, flashParams, embedAttributes) {

  try {
      twister_view_dpv_so = new SWFObject(swfLocation, "twister_view_dpv_AMPlayerProd", swfWidth, swfHeight, flashVersion, bgColor);	   

      for (var v in swfParams) {
        twister_view_dpv_so.addVariable(v, encodeURIComponent(swfParams[v]));
      }
      for (var v in flashParams) {
        twister_view_dpv_so.addParam(v, flashParams[v]);
      }
      for (var v in embedAttributes) {
        twister_view_dpv_so.setAttribute(v, embedAttributes[v]);
      }

      twister_view_dpv_so.addVariable("swfEmbedTime", new Date().getTime());

      var agt = navigator.userAgent;
       
      var reFirefox = new RegExp("firefox/", "i");
      var regx = agt.split(reFirefox);
      var ffVersion = 8;
      if (regx[1]) {
          var pts = regx[1].split(/[.]/);
          ffVersion = parseFloat(pts[0] + "." + pts[1]);
      }
      var minorVersion = parseFloat(navigator.ap);
      if ((navigator.appVersion.indexOf("Mac")!=-1) || (ffVersion < 1.5)) {
          twister_view_dpv_so.setAttribute("height", swfHeight);
          twister_view_dpv_so.setAttribute("width", swfWidth);
          twister_view_dpv_so.addVariable("oldFirefox", "1");	     
      }

      if (twister_view_dpv_so.write("twister_view_dpv_flashcontent")) {
          fp_resizePlayerSpace('twister_view_dpv_', 
                               320, 
                               283);
      }
      else {
          var nsPrefix = 'twister_view_dpv_';

          fp_resizePlayerSpace(nsPrefix, 0, 0);
          fp_rewriteDiv(nsPrefix, "flashcontent", '<br/>To view this video download <a target="_blank" href="http://www.macromedia.com/go/getflashplayer" target="_top">Flash Player</a> (version 9.0.115 or higher)');
      }
  } catch(err) {
      fp_resizePlayerSpace('twister_view_dpv_', 0, 0);
  }
  return false;
}



function _twister_view_dpv_loadSwf(movieID, playerVersion) {
    var swfLocation = "http://g-ecx.images-amazon.com/images/G/01/am3/20120510035744301/AMPlayer._V148501545_.swf";
    var swfWidth = "320";
    var swfHeight = "100%";
    var flashVersion = "9.0.115";
    var bgColor = "#FFFFFF";

    var swfParams = {};
    var flashParams = {};
    var embedAttributes = {};
    swfParams.amazonPort = "80";
    swfParams.allowFullScreen = "true";
    swfParams.nsPrefix = "twister_view_dpv_";
    swfParams.locale = "en_US";
    swfParams.logUrl = "gp/mpd/l";
    swfParams.canResize = "1";
    swfParams.autoPlay = "1";
    swfParams.sessionId = "180-8670033-0579709";
    swfParams.marketplaceID = "ATVPDKIKX0DER";
    swfParams.salign = "LT";
    swfParams.asin = "B002P8T0L0";
    swfParams.preset = "detail";
    swfParams.mediaObjectId = "mY1JGAAFS5KXD";
    swfParams.autoPlayTimer = "";
    swfParams.mediaObjectIDList = "mY1JGAAFS5KXD";
    swfParams.playIsUserGenerated = "1";
    swfParams.permUrl = "gp/mpd/permalink";
    swfParams.xmlUrl = "http://www.amazon.com/gp/mpd/getplaylist-v2/mY1JGAAFS5KXD/180-8670033-0579709";
    swfParams.amazonServer = "www.amazon.com";
    swfParams.scale = "noscale";
    swfParams.amazonAmbLink = "/foo.html/ref=XLX/180-8670033-0579709";
    flashParams.allowFullScreen = "true";
    flashParams.salign = "LT";
    flashParams.bgcolor = "#FFFFFF";
    flashParams.allowScriptAccess = "always";
    flashParams.quality = "high";
    flashParams.wmode = "transparent";
    embedAttributes.title = "Flash Player";

    if (movieID){
      if (playerVersion == '3'){
        swfLocation = "http://g-ecx.images-amazon.com/images/G/01/am3/20120510035744301/AMPlayer._V148501545_.swf";
        swfParams.mediaObjectIDList = movieID;
      } else {
        swfLocation = "http://g-ecx.images-amazon.com/images/G/01/am3/legacy/20101014901/AMPlayerProd._V197491872_.swf";
      }
      swfParams.xmlUrl = "http://www.amazon.com/gp/mpd/getplaylist-v2/" + movieID + "/180-8670033-0579709";
      flashParams.wmode = "transparent";
      swfParams.autoPlay = "1";
    }

    if (document.getElementById('twister_view_dpv_duration')) {
      document.getElementById('twister_view_dpv_duration').style.display='none';
    }
    twister_view_dpv_loadSwf_Core(swfLocation, swfWidth, swfHeight, flashVersion, bgColor, swfParams, flashParams, embedAttributes);

  return false;
}

function twister_view_dpv_loadSwf(movieID, playerVersion) {
if (typeof amznJQ != "undefined") {
   amznJQ.available('swfobjectJS', function() { _twister_view_dpv_loadSwf(movieID, playerVersion); });
   return false;
} else {
   return _twister_view_dpv_loadSwf(movieID, playerVersion);
}
}


	
		// ]]>
	</script>


