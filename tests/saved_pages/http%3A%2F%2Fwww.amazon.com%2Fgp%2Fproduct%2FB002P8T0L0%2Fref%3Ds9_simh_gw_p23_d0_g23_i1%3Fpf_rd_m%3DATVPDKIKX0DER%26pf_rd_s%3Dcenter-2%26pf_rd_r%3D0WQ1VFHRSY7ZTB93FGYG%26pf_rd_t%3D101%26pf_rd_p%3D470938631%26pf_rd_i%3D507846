<div class="quorus-product">
  


<script type="text/javascript">
  amznJQ.available('twister', function() {
    if (!(typeof DetailPageFramework != 'undefined' && DetailPageFramework.registerFeature))
      return;

    DetailPageFramework.registerFeatureConfig('quorus-product-attributes', { suppressDefaultBehavior: true });

    var parentUrl = "http://www.amazon.com/Lady-Gaga-Heartbeats-In-Ear-Headphones/dp/B002P8T0L0/ref=quorus/180-8670033-0579709";

    function setQuorusItem(asin) {
      QUORUS.use(function(Q) {
        Q.setItem({
          identifier:     asin || "B002P8T0L0",
          identifierType: 'ASIN',
          category: "ce_display_on_website",
          nameSelector:   '#btAsinTitle',
          imageSelector:  'img#main-image, img#prodImage, #prodImageCell img',
          url:            asin ? urlForASIN(asin) : parentUrl
        });
      });
    }

    function updateQuorusItem(dpState) {
      var asin = getSelectedASIN(dpState);
      if (asin) setQuorusItem(asin);
    }

    function getSelectedASIN(dpState) {
      if (dpState.current_asin) return dpState.current_asin;

      if (dpState.asin_variation_values && dpState.selected_variation_values) {
        var asins = dpState.asin_variation_values,
             dims = dpState.selected_variation_values,
             asin, dim, match;

        for (asin in asins) {
          match = true; 

          for (dim in dims) {
            if (dims[dim] < 0) continue;
            if (dims[dim] != asins[asin][dim]) {
              match = false; break;
            }
          }

          if (match) return asin;
        }
      }
    }

    function urlForASIN(asin) {
      return parentUrl.replace(/\/[A-Z0-9]{10}(\/|$)/, '/'+asin+'$1');
    }

    DetailPageFramework.registerCallback('asin_select', 'quorus-product-attributes', updateQuorusItem);
    DetailPageFramework.registerCallback('asin_deselect', 'quorus-product-attributes', updateQuorusItem);

    setQuorusItem();
  });
</script>

</div>







  
    
    


