<script type="text/javascript" >
    amznJQ.available
    ('jQuery', function() {
        function changeBuyingOptions(show, hide) {
            var buyingOptionsContainer = jQuery('#buyingOptionsContainer');
            var heightBefore = buyingOptionsContainer.height();
            var heightAfter = jQuery('#' + show + 'BuyingOptions').height();
            jQuery('#' + show + 'DeliveryInputBox').addClass('selected');
            jQuery('#' + hide + 'DeliveryInputBox').removeClass('selected');
            jQuery('#' + show + 'DeliveryInputButton').attr('checked', 'true');
            if ( show == 'sns') {
                jQuery('#snsFrequencySelection').removeAttr('disabled');
            } else {
                jQuery('#snsFrequencySelection').attr('disabled', 'disabled');
            }
            jQuery('#' + hide + 'BuyingOptions').hide();
            buyingOptionsContainer.height(heightBefore);
            buyingOptionsContainer.stop().animate({height:heightAfter}, 'fast', function()
                                                  {
                                                      jQuery('#' + show + 'BuyingOptions').fadeIn('fast');
                                                      buyingOptionsContainer.css('height', '');
                                                  });
        };
        jQuery("#standardDeliveryLabel, #snsDeliveryLabel, #standardDeliveryInputButton, #snsDeliveryInputButton").click(
            function() {
                var id = jQuery(this).attr('id');
                if ((id == 'standardDeliveryLabel' || id == 'standardDeliveryInputButton') && jQuery('#standardBuyingOptions:visible').length == 0) {
                    changeBuyingOptions('standard', 'sns');
                } else if ((id == 'snsDeliveryLabel' || id == 'snsDeliveryInputButton') && jQuery('#snsBuyingOptions:visible').length == 0) {
                    changeBuyingOptions('sns', 'standard');
                }
                
            });
        jQuery('#buyboxOptionsNonJSLink').remove();
        jQuery('#standardDeliveryLabel, #snsDeliveryLabel').addClass('labelContainer');

            jQuery('#standardDeliveryInputButton').attr('checked', 'true');
            jQuery('#snsFrequencySelection').attr('disabled', 'disabled');
     });

</script>
</div></div>
  
  


















